button6.MouseButton1Click:Connect(function()
    local humanoid = game.Players.LocalPlayer.Character.Humanoid
    if lol3 == 1 then
        local anim = Instance.new("Animation")
        anim.AnimationId = "rbxassetid://121572214"
        floattrack = humanoid:LoadAnimation(anim)
        floattrack:Play()
        button6.Text = "floating head(on)"
        lol3 = 0
    else
        if floattrack then
            floattrack:Stop()
        end
        button6.Text = "floating head(off)"
        lol3 = 1
    end
end)

button7.MouseButton1Click:Connect(function()
local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://45834924"
local track = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(anim)
track:Play()
end)

button8.MouseButton1Click:Connect(function()
    local humanoid = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    if not humanoid then return end

    if lol4 == 1 then
        local anim = Instance.new("Animation")
        anim.AnimationId = "rbxassetid://259438880"
        armtrack = humanoid:LoadAnimation(anim)
        armtrack:Play()
        armtrack:AdjustSpeed(2)
        button8.Text = "arm rutine(on)"
        lol4 = 0
    else
        if armtrack and armtrack.IsPlaying then
            armtrack:Stop()
        end
        button8.Text = "arm rutine(off)"
        lol4 = 1
    end
end)

next.MouseButton1Click:Connect(function()
	button.Visible = false
	button1.Visible = false
	button2.Visible = false
	button3.Visible = true
	button4.Visible = true
	button5.Visible = true
	next.Visible = false
	back.Visible = true
	next1.Visible = true
end)

back.MouseButton1Click:Connect(function()
button.Visible = true
button1.Visible = true
button2.Visible = true
button3.Visible = false
button4.Visible = false
button5.Visible = false
next.Visible = true
back.Visible = false
next1.Visible = false
end)

credit.MouseButton1Click:Connect(function()
if frame1.Visible then
	frame1.Visible = false
else
	frame1.Visible = true
end
end)

next1.MouseButton1Click:Connect(function()
button.Visible = false
button1.Visible = false
button2.Visible = false
button3.Visible = false
button4.Visible = false
button5.Visible = false
button6.Visible = true
button7.Visible = true
button8.Visible = true
next.Visible = false
back.Visible = false
next1.Visible = false
back1.Visible = true
end)

back1.MouseButton1Click:Connect(function()
button3.Visible = true
button4.Visible = true
button5.Visible = true
button6.Visible = false
button7.Visible = false
button8.Visible = false
next1.Visible = true
back.Visible = true
back1.Visible = false
end)
