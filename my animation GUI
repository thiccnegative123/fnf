--button functions


button.MouseButton1Click:Connect(function()
    local humanoid = game.Players.LocalPlayer.Character.Humanoid
    
    if lol == 1 then
        local anim = Instance.new("Animation")
        anim.AnimationId = "rbxassetid://287325678"
        crouchTrack = humanoid:LoadAnimation(anim)
        crouchTrack:Play()
humanoid.WalkSpeed = 10
        lol = 0
        button.Text = "Crouch (on)"
    else
        if crouchTrack then
            crouchTrack:Stop()
humanoid.WalkSpeed = 16
        end
        lol = 1
        button.Text = "Crouch (off)"
    end
end)

button1.MouseButton1Click:Connect(function()
if lol1 == 1 then
local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://33796059"
insanetrack = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(anim)
insanetrack:Play()
button1.Text = "Insane(on)"
insanetrack:AdjustSpeed(100000)
lol1 = 0
else
if insanetrack then
insanetrack:Stop()
button1.Text = "Insane(off)"
end
lol1 = 1
end
end)

button2.MouseButton1Click:Connect(function()
local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://35154961"
local track = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(anim)
track:Play()
end)

button3.MouseButton1Click:Connect(function()
local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://126753849"
local track = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(anim)
track:Play()
track:AdjustSpeed(5)
wait(0.34)
local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://126753849"
local track = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(anim)
track:Play()
track:AdjustSpeed(5)
wait(0.34)
local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://126753849"
local track = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(anim)
track:Play()
track:AdjustSpeed(5)
end)

